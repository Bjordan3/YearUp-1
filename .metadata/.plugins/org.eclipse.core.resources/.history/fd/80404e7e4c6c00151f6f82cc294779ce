
public class Person {
	String NName = null;
	String NPhoneNumber = null;
	String NAddress = null;
	int NAge;
	public static void main(String[] args) {
		Person Jacquawn = new Person("Jacquawn", "678-357-8435",
				"987 Burns Drive Atlanta, Georgia 30310", 21);
		System.out.println(
				"This is " + Jacquawn.NName);
		System.out.println(
				"This is Jacquawns number " + Jacquawn.NPhoneNumber);
		System.out.println(
				"This is Jacquawns address " + Jacquawn.NAddress);
		System.out.println(
				"This is Jacquawns age " + Jacquawn.NAge);
	}
	Person(String name) {
		NName = name;
	}
	Person(String name, String number) {
		NName = name;
		NPhoneNumber = number;
	}
	Person(String name, String number, int age) {
		NName = name;
		NPhoneNumber = number;
		NAge = age;
	}
	Person(String name, String number, String address, int age) {
		NName = name;
		NPhoneNumber = number;
		NAddress = address;
		NAge = age;
	}
}
